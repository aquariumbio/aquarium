<script type="text/ng-template" id="title">
  <div class='content content-title'>
    {{content_value(line)}}
  </div>
</script>

<script type="text/ng-template" id="note">
  <div class='content content-note'>
    {{content_value(line)}}
  </div>
</script>

<script type="text/ng-template" id="select">
  <div class='content content-select'>
    <b>{{content_value(line).var}}</b> = {{content_value(line).label}} from 
    [ <span ng-repeat="choice in content_value(line).choices">{{choice}}<span ng-if="!$last">, </span></span> ]
  </div>
</script>

<script type="text/ng-template" id="input">
  <div class='content content-note'>
    <b>{{content_value(line).var}}</b> = {{content_value(line).label}} ({{content_value(line).type}})
  </div>
</script>

<script type="text/ng-template" id="bullet">
