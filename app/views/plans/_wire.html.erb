<polyline ng-attr-class="wire-hitline"
          ng-mousedown="wireMouseDown($event, wire)" 
          ng-attr-points="{{wire.path}}">
</polyline>

<polyline ng-attr-class="{{wire_class(wire)}}"
          ng-mousedown="wireMouseDown($event, wire)" 
          ng-attr-points="{{wire.path}}">
</polyline>

<path ng-attr-class="{{wire_arrow_class(wire)}}"
      ng-attr-d="{{wire.arrowhead}}">
</path>



